generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_CONNECTION")
}

model UserMailService {
  id                      BigInt  @id @default(autoincrement())
  document                String
  To_DebtCollectorName    String
  To_DebtCollectorAddress String
  To_DebtCollectorCity    String
  To_DebtCollectorState   String
  To_DebtCollectorZipCode String
  From_ContactFullName    String
  From_ContactAddress     String
  From_ContactCity        String
  From_ContactState       String
  From_ContactZipCode     String
  pdfLink                 String?
}
